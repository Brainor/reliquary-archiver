{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "rustc: debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/reliquary-archiver.exe",
            "args": [
                "--pcap",
                "${workspaceFolder}/res/login.pcap",
                "--resource-path",
                "${workspaceFolder}/res/json",
            ],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "environment": [
                {
                    "name": "Path",
                    "value": "${env:Path};C:\\Windows\\System32\\Npcap"
                },
                {
                    "name": "http_proxy",
                    "value": "127.0.0.1:,7890"
                },
                {
                    "name":"RUST_BACKTRACE","value":"full"
                }
            ],
            "preLaunchTask": "rust: cargo build"
        }
    ]
}